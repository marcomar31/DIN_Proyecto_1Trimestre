<Window x:Class="Proyecto_Final.AgregarClienteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto_Final.Componentes"
        mc:Ignorable="d"
        Title="Añadir Cliente" MinWidth="854" MinHeight="480"
        ResizeMode="CanResizeWithGrip" SizeToContent="WidthAndHeight">
    <Window.Icon>
        <BitmapImage UriSource="/Favicon_Maleta_SinFondo.png"/>
    </Window.Icon>
    <Window.Background>
        <ImageBrush ImageSource="/Background_Paisaje.png" Stretch="UniformToFill"/>
    </Window.Background>
    <Viewbox MaxHeight="750" MaxWidth="800">
        <Viewbox.OpacityMask>
            <ImageBrush ImageSource="/Background_Paisaje.png"/>
        </Viewbox.OpacityMask>
        <local:ComponentePersonalizado_Cliente x:Name="componenteCliente" Titulo="NUEVO CLIENTE" />
    </Viewbox>
</Window>
